class Area {
  
  public int aWidth;
  public int aHeight;
  public int x;
  public int y;

  
  public Area(int aWidth, int aHeight, int x, int y){
    this.aWidth = aWidth;
    this.aHeight = aHeight;
  }
  
  public int getWidth(){
    return aWidth;
  }
  
  public int getHeight(){
   return aHeight; 
  }
  public int getX(){
    return x;
  }
  
  public int getY(){
   return y; 
  }
  
}
